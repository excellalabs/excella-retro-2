{
  "name": "excella-retro",
  "version": "2.1.0",
  "license": "MIT",
  "angular-cli": {},
  "homepage": "https://retro.excella.com",
  "private": true,
  "imageRepo": "090999229429.dkr.ecr.us-east-1.amazonaws.com/excella-retro-ui",
  "imageName": "excella-retro-ui",
  "imagePort": "8080",
  "awsRegion": "us-east-1",
  "awsEcsCluster": "staging-tests",
  "awsService": "excella-retro-ui",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:build": "with-package docker image build . -t pkg.imageRepo:pkg.version && npm run docker:tag",
    "docker:tag": "with-package docker image tag pkg.imageRepo:pkg.version pkg.imageRepo:latest",
    "docker:run": "npm run docker:clean && npm run docker:runHelper",
    "docker:runHelper": "with-package docker run --name pkg.imageName -d -p pkg.imagePort:3000 pkg.imageRepo",
    "docker:publish": "with-package docker image push pkg.imageRepo",
    "docker:clean": "with-package docker rm -f pkg.imageName || true",
    "docker:taillogs": "with-package docker logs -f pkg.imageName",
    "docker:debug": "npm run docker:build && npm run docker:run && sleep 1 && with-package open 'http://localhost:pkg.imagePort' && npm run docker:taillogs",
    "aws:login": "AWS_REGION=$(with-package echo 'pkg.awsRegion' | tail -1 | awk '{print $NF}') && eval $(aws ecr get-login --no-include-email --region $AWS_REGION)",
    "aws:deploy": "with-package docker run --env-file ./.env silintl/ecs-deploy -c pkg.awsEcsCluster -n pkg.awsService -i pkg.imageRepo:latest -r pkg.awsRegion --timeout 1000",
    "aws:release": "npm run aws:login && npm run docker:publish && npm run aws:deploy"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "2.0.0-beta.6",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular-2-local-storage": "^1.0.1",
    "angularfire2": "2.0.0-beta.8",
    "core-js": "^2.4.1",
    "firebase": "^3.6.10",
    "hammerjs": "^2.0.8",
    "ngx-clipboard": "^5.0.3",
    "rxjs": "^5.0.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "2.0.0",
    "tslint": "^4.5.0",
    "typescript": "~2.2.0",
    "with-package": "^0.2.0"
  },
  "authors": [
    "Sam Blissard<sam.blissard@excella.com>",
    "Alex Hoffman<alex.hoffman@excella.com>",
    "Cameron Ivey<cameron.ivey@excella.com>",
    "Brendan Sawyer<brendan.sawyer@excella.com>"
  ]
}
