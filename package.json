{
  "name": "excella-retro",
  "version": "2.1.0",
  "license": "MIT",
  "homepage": "https://retro.excella.com",
  "imageRepo": "090999229429.dkr.ecr.us-east-1.amazonaws.com/excella-retro-ui",
  "imageName": "excella-retro-ui",
  "imagePort": "8080",
  "awsRegion": "us-east-1",
  "awsEcsCluster": "excella-prod-ecs-cluster",
  "awsService": "excella-retro-prod",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:build": "npm run build -- --prod && with-package docker image build . -t pkg.imageRepo:latest",
    "docker:run": "npm run docker:clean && npm run docker:runHelper",
    "docker:runHelper": "with-package docker run --name pkg.imageName -d -p pkg.imagePort:3000 pkg.imageRepo",
    "docker:publish": "with-package docker image push pkg.imageRepo:latest",
    "docker:clean": "with-package docker rm -f pkg.imageName || true",
    "docker:taillogs": "with-package docker logs -f pkg.imageName",
    "docker:debug": "npm run docker:build -- --file Dockerfile.dev && npm run docker:run && sleep 1 && with-package open 'http://localhost:pkg.imagePort' && npm run docker:taillogs",
    "aws:login": "with-package aws ecr get-login --no-include-email --region pkg.awsRegion",
    "aws:deploy": "with-package docker run --env-file ./.env silintl/ecs-deploy -c pkg.awsEcsCluster -n pkg.awsService -i pkg.imageRepo:latest -r pkg.awsRegion --timeout 1000",
    "aws:release": "npm run aws:login && npm run docker:publish && npm run aws:deploy"
  },
  "dependencies": {
    "@angular-devkit/build-optimizer": "0.0.38",
    "@angular-devkit/core": "0.0.25",
    "@angular-devkit/schematics": "0.0.48",
    "@angular/animations": "^8.2.9",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^8.2.9",
    "@angular/compiler": "^8.2.9",
    "@angular/core": "^8.2.9",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/forms": "^8.2.9",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^8.2.9",
    "@angular/platform-browser-dynamic": "^8.2.9",
    "@angular/router": "^8.2.9",
    "@ngtools/json-schema": "^1.1.0",
    "@ngtools/webpack": "^1.9.3",
    "@schematics/angular": "^8.3.8",
    "@schematics/schematics": "0.0.14",
    "angular-2-local-storage": "^1.0.1",
    "angularfire2": "2.0.0-beta.8",
    "core-js": "^2.4.1",
    "firebase": "^3.6.10",
    "hammerjs": "^2.0.8",
    "nanoid": "^0.2.2",
    "ngx-clipboard": "^5.0.3",
    "rxjs": "^6.5.3",
    "webpack": "^3.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/cli": "8.3.8",
    "@angular/compiler-cli": "^8.2.9",
    "@angular/language-service": "^8.2.9",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "2.0.0",
    "tslint": "^4.5.0",
    "typescript": "^3.5.3",
    "with-package": "^0.2.0"
  },
  "authors": [
    "Sam Blissard<sam.blissard@excella.com>",
    "Alex Hoffman<alex.hoffman@excella.com>",
    "Cameron Ivey<cameron.ivey@excella.com>",
    "Brendan Sawyer<brendan.sawyer@excella.com>"
  ]
}
