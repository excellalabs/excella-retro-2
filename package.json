{
  "name": "excella-retro",
  "version": "2.2.0",
  "repository": {
    "url": "https://github.com/excellalabs/excella-retro-2"
  },
  "license": "MIT",
  "homepage": "https://retro.excella.com",
  "config": {
    "imageRepo": "090999229429.dkr.ecr.us-east-1.amazonaws.com/excella-retro-ui",
    "imageName": "excella-retro-ui",
    "imagePort": "8080",
    "awsRegion": "us-east-1",
    "awsEcsCluster": "excella-prod-ecs-cluster",
    "awsService": "excella-retro-prod"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "standardize": "import-sort --write \"**/*.ts\" && prettier --write \"**/*.ts\" && js-beautify --editorconfig \"src/**/*.html\"",
    "docker:build": "npm run build -- --prod && cross-conf-env docker image build . -t $npm_package_config_imageRepo:latest",
    "docker:run": "npm run docker:clean && npm run docker:runHelper",
    "docker:runHelper": "cross-conf-env docker run --name $npm_package_config_imageName -d -p $npm_package_config_imagePort:3000 $npm_package_config_imageRepo",
    "docker:publish": "cross-conf-env docker image push $npm_package_config_imageRepo:latest",
    "docker:clean": "cross-conf-env docker rm -f $npm_package_config_imageName || true",
    "docker:taillogs": "cross-conf-env docker logs -f $npm_package_config_imageName",
    "docker:debug": "npm run docker:build -- --file Dockerfile.dev && npm run docker:run && sleep 1 && cross-conf-env open 'http://localhost:$npm_package_config_imagePort' && npm run docker:taillogs",
    "aws:login": "cross-conf-env aws ecr get-login --no-include-email --region $npm_package_config_awsRegion",
    "aws:deploy": "cross-conf-env docker run --env-file ./.env silintl/ecs-deploy -c $npm_package_config_awsEcsCluster -n $npm_package_config_awsService -i $npm_package_config_imageRepo:latest -r $npm_package_config_awsRegion --timeout 1000",
    "aws:release": "npm run aws:login && npm run docker:publish && npm run aws:deploy"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.803.8",
    "@angular-devkit/build-optimizer": "0.803.8",
    "@angular-devkit/core": "8.3.8",
    "@angular-devkit/schematics": "8.3.8",
    "@angular/animations": "^8.2.9",
    "@angular/cdk": "^8.2.2",
    "@angular/common": "^8.2.9",
    "@angular/compiler": "^8.2.9",
    "@angular/core": "^8.2.9",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.9",
    "@angular/material": "^8.2.2",
    "@angular/platform-browser": "^8.2.9",
    "@angular/platform-browser-dynamic": "^8.2.9",
    "@angular/router": "^8.2.9",
    "@ngtools/json-schema": "^1.1.0",
    "@ngtools/webpack": "^8.3.8",
    "@schematics/angular": "^8.3.8",
    "@schematics/schematics": "0.803.8",
    "angular-2-local-storage": "^3.0.2",
    "core-js": "^3.2.1",
    "firebase": "^6.6.2",
    "hammerjs": "^2.0.8",
    "nanoid": "^2.1.2",
    "ngx-clipboard": "^12.2.1",
    "rxjs": "^6.5.3",
    "webpack": "^4.41.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/cli": "8.3.8",
    "@angular/compiler-cli": "^8.2.9",
    "@angular/language-service": "^8.2.9",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "3.4.2",
    "@types/node": "^12.7.11",
    "codelyzer": "~5.1.2",
    "cross-conf-env": "^1.1.2",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "js-beautify": "^1.10.2",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^1.18.2",
    "prettier-tslint": "^0.4.2",
    "protractor": "~5.4.2",
    "ts-node": "8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.5.3"
  },
  "authors": [
    "Sam Blissard<sam.blissard@excella.com>",
    "Alex Hoffman<alex.hoffman@excella.com>",
    "Cameron Ivey<cameron.ivey@excella.com>",
    "Brendan Sawyer<brendan.sawyer@excella.com>",
    "Doguhan Uluca<doguhan.uluca@excella.com"
  ],
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "options": {}
    }
  },
  "prettier": {
    "printWidth": 90,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript",
    "jsxBracketSameLine": true
  }
}
